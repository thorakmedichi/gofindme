{
  "private": true,
  "name": "@sketchpad/eslint-config",
  "version": "0.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "eslintConfig": {
    "extends": "./src/index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "scripts": {
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
