{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    },
    "skipLibCheck": true
  },
  "include": ["next-env.d.ts",
    "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
